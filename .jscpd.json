{
  "threshold": 5,
  "reporters": ["console", "json"],
  "output": "./reports/jscpd",
  "pattern": "src/**/*.{ts,tsx}",
  "ignore": [
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/coverage/**",
    "**/*.d.ts",
    "**/types/**"
  ],
  "minLines": 10,
  "minTokens": 50,
  "formatsExts": {
    "typescript": ["ts", "tsx"]
  }
}
